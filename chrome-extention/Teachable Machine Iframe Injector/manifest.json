{
  "manifest_version": 3,
  "name": "Teachable Machine Iframe Injector",
  "version": "1.0",
  "permissions": ["scripting"],
  "host_permissions": [
    "https://teachablemachine.withgoogle.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://teachablemachine.withgoogle.com/*"],
      "js": ["inject.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ]
}
